# Domain
DOMAIN=your-domain.example.com
ACME_EMAIL=admin@example.com

# Database (CHANGE THIS!)
DB_PASSWORD=your-secure-database-password

# JWT (CHANGE THIS! min 32 chars)
SECRET_KEY=change-me-to-random-string-min-32-characters

# OIDC (ADFS) - Get these from your identity provider
OIDC_DISCOVERY_URL=https://your-sso-server/adfs/.well-known/openid-configuration
OIDC_CLIENT_ID=your-client-id-here
OIDC_CLIENT_SECRET=your-client-secret-here
OIDC_REDIRECT_URI=https://your-domain.example.com/auth/sso/callback

# CORS
CORS_ORIGINS=https://your-domain.example.com
